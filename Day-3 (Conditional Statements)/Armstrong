#include <iostream>
using namespace std;
int main() {
int num,rem,sum=0;
cout << "Give a Number:";
cin >> num;
int real_num=num;
while(num>0){
    rem=num%10;
    sum=sum+(rem*rem*rem);
    num=num/10;
}


if(real_num==sum){
    cout << "Given number is Amstrong";
}
else{
    cout << "Given number is not a Amstrong";
}

    return 0;
}
